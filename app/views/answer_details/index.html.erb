<p id="notice"><%= notice %></p>

<h1>Answer Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_details.each do |answer_detail| %>
      <tr>
        <td><%= link_to 'Show', answer_detail %></td>
        <td><%= link_to 'Edit', edit_answer_detail_path(answer_detail) %></td>
        <td><%= link_to 'Destroy', answer_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer Detail', new_answer_detail_path %>
